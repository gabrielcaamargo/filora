{
  "name": "docuscanapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android --device",
    "test": "jest",
    "coverage": "jest --coverage",
    "ios": "expo run:ios --device --no-install",
    "web": "expo start --web",
    "prepare": "husky",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@hookform/resolvers": "^5.2.1",
    "@react-native-firebase/app": "^23.3.1",
    "@react-native-firebase/auth": "^23.3.1",
    "@react-native-firebase/crashlytics": "^23.3.1",
    "@react-native-firebase/firestore": "^23.3.1",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.89.0",
    "commitlint": "^19.8.1",
    "expo": "~53.0.22",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-status-bar": "~2.2.3",
    "immer": "^10.1.3",
    "react": "19.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.6",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "^0.5.1",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/json": "^0.13.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^13.2.3",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "reactotron-react-native": "^5.1.17",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|immer)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "index"
    ],
    "collectCoverageFrom": [
      "src/{components,utils,hooks,domain,database}/**/*.{ts,js,tsx,jsx}"
    ],
    "moduleDirectories": [
      "node_modules",
      "./src/test"
    ],
    "modulePathIgnorePatterns": [
      ".*/mockedData/.*"
    ],
    "setupFiles": [
      "<rootDir>/src/test/jestSetup.tsx"
    ]
  },
  "private": true
}
